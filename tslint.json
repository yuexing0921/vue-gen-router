{
    "extends": [
        "tslint:recommended",
		"tslint-config-prettier"
    ],
	"linterOptions": {
		"exclude": ["dist/**/*.js", "bin/*.js"]
	},
    "rules":{
        "interface-name" : [true, "never-prefix"],
        "no-unused-expression":[
			true,
			"allow-fast-null-checks",
			"allow-new"
        ],
        "object-literal-sort-keys":false,
        "no-console": false,
        "no-empty-interface":false
    }
}
